{
    "language": "en-GB",
    "author": "CZghost",
    "content": {
        "language": "English (United Kingdom)",
        "events": {
            "levelup": {
                "message": "[user] has leveled up!",
                "embed": {
                    "caption": "Level up!",
                    "content": "New XPs: [xp]; New level: [lvl]"
                }
            },
            "guildjoin": {
