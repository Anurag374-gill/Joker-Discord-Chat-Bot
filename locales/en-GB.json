            }
        },
        "commands": {
            "whois": {
                "help": {
                    "description": "Returns user information",
                    "args": {
                        "user_reference": "Reference to user whose informations you wish to display"
                    }
                },
                "errors": {
                    "nouser": "That user doesn't exist on this server, [author]."
                },
                "query": {
                    "text_none": "(none)",
                    "embed": {
                        "footer": "Queried by [author]",
                        "title": "User card",
