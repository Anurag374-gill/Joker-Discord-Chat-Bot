                            "caption": "Server member info",
                            "description": {
                                "nick": "Server nick",
                                "joined": "Member joined at",
                                "roles": "Assigned roles"
                            }
                        },
                        "user_account_info": {
                            "caption": "User account info",
                            "description": {
                                "id": "User ID",
                                "username": "Username",
                                "tag": "Discord TAG",
                                "created": "Account created at"
                            }
                        },
                        "activity": {
                            "caption": {
                                "playing": "Currently playing",
                                "streaming": "Currently streaming",
                                "listening": "Currently listening",
                                "watching": "Currently watching",
                                "doing": "Currently doing"
                            },
                            "description": "Name"
                        },
                        "moderation": {
                            "warning": {
                                "caption": "Moderation informations",
